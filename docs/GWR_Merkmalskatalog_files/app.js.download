    <script>
			// Get the button
			const goTopBtn = document.getElementById("goTopBtn");

			window.addEventListener("scroll", function() {
			  if (window.scrollY > 1000) {
			    goTopBtn.style.opacity = "1";        // fade in
			    goTopBtn.style.pointerEvents = "auto"; // clickable
			  } else {
			    goTopBtn.style.opacity = "0";        // fade out
			    goTopBtn.style.pointerEvents = "none"; // not clickable
			  }
			});

			</script>
			
    <script>        
